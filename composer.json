{
    "name": "adriaanzon/laravel-gfm-docs",
    "description": "Github Flavored Markdown documentation viewer for Laravel",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "league/commonmark": "^2.4",
        "laravel/framework": "^9|^10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AdriaanZon\\LaravelGfmDocs\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adriaan Zonnenberg"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AdriaanZon\\LaravelGfmDocs\\GfmDocsServiceProvider"
            ]
        }
    }
}
